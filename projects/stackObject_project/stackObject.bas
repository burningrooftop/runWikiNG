' stackObject' by Neal Collins, 2008.'' This code is public domain.stackSize = 500   ' Size of the stackdim stack$(stackSize)stackTop = -1global stack$, stackSize, stackTop' Initialise the object' ---------------------function initialise()  stackTop = -1end function' Push an item onto the stack, return 0 if the stack is full, 1 otherwise' -----------------------------------------------------------------------function push(item$)   if stackTop + 1 < stackSize then    stackTop = stackTop + 1    stack$(stackTop) = item$    push = 1  end ifend function' Return the top item on the stack without removing it' ----------------------------------------------------function peek$()  if stackTop >= 0 then peek$ = stack$(stackTop) end function' Return the top item on the stack and remove it from the stack' -------------------------------------------------------------function pop$()   if stackTop >= 0 then     pop$ = stack$(stackTop)     stackTop = stackTop - 1  end ifend function' Return the bottom most item on the stack without removing it' ------------------------------------------------------------function peekHead$()  if stackTop >= 0 then peekHead$ = stack$(0)end function' Return 1 if the stack is not empty, otherwise 0' -----------------------------------------------function hasdata()   if stackTop >= 0 then     hasdata = 1  else     hasdata = 0   end if end function